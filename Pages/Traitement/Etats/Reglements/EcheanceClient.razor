@page "/et-echeances-clients"
@attribute [Authorize(Roles = "Super Admin,Admin,Direction,Commercial")]



@if (IsLoaded)
{
    <Card_1 Title="ECHEANCES CLIENTS">
        <div class="grid-container no-border">
            <DetailsEcheance IsClients="true" ClassHeight="grid-84" DO_Domaine="0"></DetailsEcheance>
        </div>
    </Card_1>
}
else
{
    <div class="center-screen">
        <Loading_1 />
    </div>
}


@code {
    bool IsLoaded = false;
    protected override async Task OnInitializedAsync()
    {
        await Task.Delay(1);
        IsLoaded = true;
    }
}